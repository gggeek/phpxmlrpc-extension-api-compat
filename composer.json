{
    "name": "phpxmlrpc/polyfill-xmlrpc",
    "description": "A pure-php reimplementation of the API exposed by the native XML-RPC extension",
    "license": "BSD-3-Clause",
    "keywords": [ "webservices", "xmlrpc", "php", "extension", "polyfill" ],
    "require":
    {
        "php": "^5.3.0 || ^7.0 || ^8.0",
        "phpxmlrpc/phpxmlrpc": "^4.4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0 || ^8.5.12",
        "yoast/phpunit-polyfills": "*",
        "ext-xmlrpc": "*"
    },
    "autoload": {
        "psr-4": {
            "PhpXmlRpc\\Polyfill\\XmlRpc\\": "src/"
        },
        "files": [
            "bootstrap.php"
        ]
    }
}
